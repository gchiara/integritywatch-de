                "affectedLaws": [
                    {
                        "shortTitle": "SGB 4",
                        "title": "Sozialgesetzbuch (SGB) Viertes Buch (IV) - Gemeinsame Vorschriften f\u00fcr die Sozialversicherung - (Artikel I des Gesetzes vom 23. Dezember 1976, BGBl. I S. 3845)",
                        "url": "https://www.gesetze-im-internet.de/sgb_4"
