        "regulatoryProjects": [
            {
                "affectedLaws": [
                    {
                        "shortTitle": "GEG",
                        "title": "Gesetz zur Einsparung von Energie und zur Nutzung erneuerbarer Energien zur W\u00e4rme- und K\u00e4lteerzeugung in Geb\u00e4uden",
                        "url": "https://www.gesetze-im-internet.de/geg"
