    "activitiesAndInterests": {
        "activity": {
            "code": "ACT_PRIVATE_ORGA",
            "de": "Privatrechtliche Organisation mit Gemeinwohlaufgaben (z. B. eingetragene Vereine, Stiftungen) (GL2022)",
            "en": "Organization under private law with public welfare tasks (e.g. registered associations, foundations) (GL2022)"
