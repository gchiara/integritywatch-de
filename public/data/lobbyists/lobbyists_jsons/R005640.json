    "activitiesAndInterests": {
        "activity": {
            "code": "ACT_PRIVATE_ORGA_V2",
            "de": "Privatrechtliche Organisation mit Anerkennung der Gemeinn\u00fctzigkeit nach Abgabenordnung",
